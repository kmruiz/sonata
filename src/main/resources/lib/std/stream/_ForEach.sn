requires std.stream.providers._Provider

entity class _ForEach(previous: _Provider, apply: (any) -> any) implements _Provider {
    let poll(): _StreamToken = {
        let token: _StreamToken = previous.poll()
        if (token.complete == false) {
            apply(token.value)
            self.poll()
        }
    }

    let close() = self.stop()
}